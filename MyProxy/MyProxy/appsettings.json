{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "login-route",
        "ClusterId": "login-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    ],
    "Clusters": {
      "login-cluster": {
        "Destinations": {
          "cluster1/destination1": {
            "Address": "https://localhost:10000"
          }
        }
      }
    }
  }
}
